{
  "root": true,
  "extends": [
    // add more generic rulesets here, such as:
    "plugin:vue/base"
  ],
  "rules": {
    "strict": 0,
    "semi": "error"
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}
